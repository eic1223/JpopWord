<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VocaFragment">

    <!--<ScrollView
        android:id="@+id/vocaFragment_scrollView_voca"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible">-->

    <LinearLayout
        android:id="@+id/vocaFragment_voca_group"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/vocaFragment_layout_voca_none"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:visibility="visible">

            <TextView
                android:id="@+id/vocaFragment_textView33"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:gravity="center"
                android:text="@string/songs_voca_none_guide"
                android:textAlignment="center" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/vocaFragment_layout_voca_is"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/vocaFragment_recyclerView_voca"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />

        </LinearLayout>
    </LinearLayout>
    <!--</ScrollView>-->

</FrameLayout>